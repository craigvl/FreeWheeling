@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}
<div class="row">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a href="/Home/Index" onclick="ShowProgress()"><img class="nav-height" src="http://bunchy.hoocreative.netdna-cdn.com/Content/Images/bunchytag.png" /></a>
            </div>
        </div>
    </div>
    <div class="container body-content">
        <div class="row">
            <p></p>
            <p><b>Whoops looks like there was a small issue with bunchy!</b></p>
            <p>Click <a href="/Home/Index"> here </a> to go back to bunchy.</p>
            <p>If this continues to happen please send an email to webmaster@bunchy.com.au to let someone at bunchy know.</p>
            <p>Please provide a brief description of the issue.</p>
            <p>Thanks from the team at Bunchy!</p>
        </div>
    </div>
</div>
