@model IEnumerable<FreeWheeling.Domain.Entities.Group>

@{
    ViewBag.Title = "Index";
}

<h2>Groups</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.ActionLink("Join", "Join", new { id = item.id })
        </td>
    </tr>
}

</table>
